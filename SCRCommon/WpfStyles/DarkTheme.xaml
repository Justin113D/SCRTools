<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style TargetType="{x:Type Button}" x:Key="ToolbarButton">
        <Setter Property="Foreground" Value="#EEE"/>
    </Style>
    <Style TargetType="{x:Type Border}" x:Key="WindowBase">
        <Setter Property="BorderBrush" Value="#333"/>
        <Setter Property="Background" Value="#333"/>
    </Style>

    <Style TargetType="{x:Type Menu}" x:Key="MenuBase">
        <Setter Property="Background" Value="#3B3B3B"/>
    </Style>

    <Style TargetType="{x:Type MenuItem}" x:Key="MenuItemBase">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Foreground" Value="#EEE"/>
    </Style>

    <Style TargetType="{x:Type Popup}">
        <Setter Property=""
    </Style>    
    
    <Style TargetType="{x:Type TreeView}" x:Key="TreeViewBase">
        <Setter Property="Background" Value="#444"/>
    </Style>

    <Style TargetType="{x:Type ContextMenu}">
        <Setter Property="Background" Value="#444"/>
    </Style>

</ResourceDictionary>